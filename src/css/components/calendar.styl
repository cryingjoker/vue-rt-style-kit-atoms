// @import '~vue2-datepicker/index.css'

.rt-calendar
  width 100%

  & .rt-calendar__cmp
    padding-bottom 2px

  &__wrap
    box-sizing border-box
    display inline-block
    padding-top 22px
    position relative
    width 100%
    height 49px

  &__input
    border none
    cursor pointer
    outline none
    width 100%

// Lib styles
.mx
  &-date-row
    .cell
      text-align center

  &-calendar-icon,
  &-icon-calendar
    display none

