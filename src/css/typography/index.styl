.{newAppName}-wrapper
  font-size 16px

a
  color inherit

.{newAppName}-numeric-list
  color main-color
  list-style-type none
  counter-reset step-counter

  .{newAppName}-dark-theme &
    color b2c-white

.{newAppName}-ordered-list__item
  .{newAppName}-dark-theme &
    color b2c-white

  &:before
    content ""
    display inline-block
    width 6px
    height 6px
    background-color main-color
    border-radius 50%
    position absolute
    top .5em
    left 7px
    @media(max-width mobile-upper-limit)
      left 6px

    .{newAppName}-dark-theme &
      background-color b2c-white05
  &--orange
    &:before
      background-color b2c-orange
  &--purple
    &:before
      background-color b2c-purple

.{newAppName}-numeric-list__item
  counter-increment step-counter

  &:before
    content counter(step-counter) "."
    min-width 28px
    color main-color
    position absolute
    left 0

    .{newAppName}-dark-theme &
      color b2c-white

.{newAppName}-ordered-list__item,
.{newAppName}-numeric-list__item,
.{newAppName}-checked-list__item
  position relative
  padding-left 32px
  .{newAppName}-ordered-list--bullet &
    padding-left: 18px
    &:before
      left: 0

  &+&
    margin-top 16px
    @media (max-width: mobile-upper-limit)
      margin-top 12px
  .rt-annotation &
    @media (max-width: mobile-upper-limit)
      margin-left -32px
      .{newAppName}-ordered-list__item,
      .{newAppName}-numeric-list__item,
      .{newAppName}-checked-list__item
        margin-left 0

.{newAppName}-numeric-list__item
  &.rt-font-paragraph
    @media(max-width laptop-upper-limit)
      padding-left 28px
  &.rt-font-small-paragraph
    padding-left 28px
    @media(max-width mobile-upper-limit)
      padding-left 24px

.{newAppName}-ordered-list__item+.{newAppName}-numeric-list__item,
.{newAppName}-numeric-list__item+.{newAppName}-ordered-list__item
  margin-top 16px

.{newAppName}-checked-list__item
  &:before
    content ''
    background-image url('data:image/svg+xml;base64,')
    position absolute
    top .5em
    width .7em
    height .6em
    left .4em
    background-repeat no-repeat
  &--purple
    &:before
      background-image url('data:image/svg+xml;base64,')
  &--orange
    &:before
      background-image url('data:image/svg+xml;base64,')

.{b2bAppName}-faq-unordered-list
  &__item
    padding vertical-space 0

    &+&
      border-top 2px solid b2c-cool-grey05-no-opacity

  .{newAppName}-numeric-list,
  .{newAppName}-ordered-list
    color main-color05

  .{newAppName}-numeric-list__item:before
    color main-color05

  .{newAppName}-ordered-list__item:before
    background-color main-color05

  .rt-annotation__title
    font-size 18px
    line-height 26px

    @media (max-width: tablet-upper-limit)
      font-size 16px
      line-height 22px

.{newAppName}-vam
  vertical-align: middle

.{newAppName}-va-top
